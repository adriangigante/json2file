    <script>
        // adjust layout to suit if takeover is present
        function verifyFullBackgroundT() {
            if ($("#full-background-image").attr("style") != null) {
                console.log("full-background-image style", $("#full-background-image").attr("style"));
                if ($("#full-background-image").css('display') === 'none') {
                    console.log("full background image display is none");
                    $('body').removeClass("full-background");
                } else
                    $("body").addClass("full-background");
            }
            else {
                console.log("full-background-image style is not present/null");
                $("body").removeClass("full-background");
            }
        }


