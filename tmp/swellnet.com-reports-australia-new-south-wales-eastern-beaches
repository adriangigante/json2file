        // Textarea has no value
        $('#swellnet-comments-form .form-submit').attr('disabled','disabled');
        console.log('disabled');
      } else {
        // Textarea has a value
        $('#swellnet-comments-form .form-submit').removeAttr('disabled');
        console.log('enabled');
      }
