          _mcn.publisher("swellnet");
          _mcn.amf.postLoadAdCall(true);
          _mcn.amf.targeting("device","Desktop");
          _mcn.amf.targeting("area", "reports/australia/new-south-wales/northern-beaches");
        });
      </script>
      
  

 



