<script>
    function loadPlayer() {
        if (camfree.match('False')) {
            if (subscribestring) {
                $('#camHolder').html($('#lockedCamVision2').html());
                $("#cambannerupgradelink")
                    .attr("href",
                        "/coastalwatch-plus/landing/introductory-offer?utm_source=cw-cams&utm_medium=banner");
            } else {
                $('#camHolder').html($('#lockedCamVision').html());
            }
            return;
        } else if (ismobile) {
            // timeout = setTimeout(showTimeout, 330000);
        } else {
            // setTimeout(showTimeout, 1200000);
        }
        $('#camHolder').css('padding-bottom', paddingForPlayer);
    }

</script>
    
