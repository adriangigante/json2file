<script language="javascript">

function addOutbrain(){
	var temp=document.getElementById('outbraincol3');
   	var clon = temp.content.cloneNode(true);
   	var targetdiv;
   	var containers=document.getElementsByClassName("gutter gOne g3Cols");
   	if(containers.length){
      	var targetcontainer=containers[containers.length-1];
      	var targetdivs=targetcontainer.getElementsByClassName("colThree");
		for(i=0;i<targetdivs.length;i++){
 			if(targetdivs[i].classList.contains("colAside")){
  				targetdiv=targetdivs[i];
				if(targetdiv!=null){
  					targetdiv.appendChild(clon);
				}
			}
   		}
	}
}

window.onload=function(){
  if(window.location.href.indexOf("offshore-swell") == -1&&window.location.href.indexOf("swell-analysis")) {
       addOutbrain();
    }
};
</script>

<script>
window.onload=function(){
    $("IMG").each(function(){
        if(!$(this).attr('img')&&$(this).attr('data-src')){
            $(this).attr('src',$(this).data('src'));
        }
    });
};
</script>




