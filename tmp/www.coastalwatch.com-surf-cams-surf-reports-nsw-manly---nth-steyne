    <script type="text/javascript">
        function loadReplay(filename) {
            var so = new SWFObject('/Content/Flash/player-licensed.swf',
                'mpl',
                '100%',
                '100%',
                '10.1.102.64');
            so.addParam('allowfullscreen', 'true');
            so.addParam('allowscriptaccess', 'always');
            so.addParam('wmode', 'opaque');
            so.useExpressInstall('../flash/expressInstall.swf');
            so.addVariable('bufferlength', '5');
            so.addVariable('file',
                "http://s3-ap-southeast-2.amazonaws.com/cw.data/replays/3500/" +
                filename +
                ".mp4");
            so.addVariable('autostart', 'true');
            so.addVariable('stretching', 'exactfit');
            so.addVariable('plugins', '');
            so.addVariable('http.startparam', 'ec_seek');
            so.addVariable('logo', 'https://www.coastalwatch.com/images/logo.png');
            so.addVariable('logo.link', 'https://www.coastalwatch.com/');
            so.addVariable('logo.hide', 'false');
            if (!so.write('camHolder')) {
                $('#camHolder')
                    .html('Flash Player 10.1 is required. <a href="http://www.adobe.com/go/getflashplayer/"><img data-src="http://www.adobe.com/images/shared/download_buttons/get_flash_player.gif" alt="Get Adobe Flash player" /></a>');
            }
        }

        function loadStill(url) {
            var holder = document.getElementById('camHolder');
            
            holder.innerHTML='<img src="' + url + '" style="width: 100%; height: 100%;" />';
        }

        $(document).ready(function() {
            loadPlayer();
        });
    </script>

    <script type="text/javascript">
        $(document).ready(function() {
            $('.tabletSlides .cameraSlidesLinksWrap').each(function() {
                var el = $(this);
                var slider = el.bxSlider({
                    slideWidth: 1120,
                    minSlides: 1,
                    maxSlides: 1,
                    pager: false,
                    slideMargin: 0
                });
                el.data('slider', slider);
            });

            $(".cameraSlidesContent li").hide();
            $(".cameraSlidesContent li:first-child").show();

            $('.camera .dropdownPanel a, .cameraSlidesLinks .dropdownPanel a').click(function(e) {
                var el = $(this);
                var display = el.data('display');

                if (display) {
                    $('.cameraSlidesLinks').hide();
                    $('.cameraSlidesLinks.' + display.class).show();
                    $('.cameraSlidesLinks.' + display.class + ' .tabletSlides .cameraSlidesLinksWrap').data('slider')
                        .reloadSlider();
                    updateTab(display);
                    updateContent(display);
                    e.preventDefault();
                }
            });

            var updateTab = function(display) {
                if (display) {
                    var tab = $('.camera a.current');
                    tab.attr('title', display.title);
                    tab.data('short-title', display.short_title);
                    tab.data('icon', display.class);
                    window.cw.resetTabs();
                    window.cw.resizeTabs();
                }
            };
            var updateContent = function(display) {
                switch (display.class) {
                case "liveSurfCam":
                    location.reload();
                    $('.cameraCorner a.last').removeClass('selected');
                    $('.cameraCorner a.first').addClass('selected');
                    break;
                case "nearbyCams":
                    break;
                case "hourlyReplays":
                    break;
                case "stillImages":
                    break;
                case "helpWithCams":
                    break;
                };
            };

            $('.cameraCorner a').click(function(e) {
                e.preventDefault();

                var clicked = $(this);
                var holder = $('#camHolder');

                if (clicked.hasClass('first')) {
                    // Show camera
                    clicked.addClass('selected');
                    $('.cameraCorner a.last').removeClass('selected');
                    holder.empty();
                    location.reload();

                } else {
                    // Show map
                    clicked.addClass('selected');
                    $('.cameraCorner a.first').removeClass('selected');

                    holder.empty();

                    var latlng = new google.maps.LatLng('-33.7909',
                        '151.287');
                    var options = {
                        zoom: 14,
                        center: latlng,
                        mapTypeId: google.maps.MapTypeId.ROADMAP,
                        mapTypeControl: true,
                        mapTypeControlOptions: {
                            style: google.maps.MapTypeControlStyle.HORIZONTAL_BAR,
                            position: google.maps.ControlPosition.TOP_CENTER
                        },
                    };
                    var map = new google.maps.Map(holder[0], options);
                    var marker = new google.maps.Marker({
                        position: latlng,
                        map: map,
                        title: 'Manly - Nth Steyne'
                    });
                }
            });

            $('#modalEditCam .displayCamList li').click(function(e) {
                var el = $(this);
                var id = el.data('id');
                var name = el.data('name');

                var sortable = $('#modalEditCam .sortable');

                if (el.hasClass('selected')) {
                    // Remove favourite
                    el.removeClass('selected');

                    sortable.sortable('destroy');
                    $('li[data-id="' + id + '"]', sortable).remove();
                    sortable.sortable();
                } else {
                    // Add favourite
                    el.addClass('selected');

                    sortable.append('<li data-id="' + id + '" data-name="' + name + '"><span>' + name + '</span></li>');
                    sortable.sortable('destroy');
                    sortable.sortable();
                }

                e.preventDefault();
            });

            $('.sortable').sortable();

            $('#modalEditCam .iClose').click(function(e) {
                var ids = [];
                $('.sortable li').each(function() {
                    ids.push($(this).data('id'));
                });

                $.ajax({
                    type: 'POST',
                    url: '/cameras/ajax/savefavourites',
                    contentType: 'application/json;  charset=UTF-8',
                    data: JSON.stringify({ ids: ids }),
                    success: function(data) {
                        $('#modalEditCam').data('overlay').close();
                        window.location.href = window.location.href;
                    },
                    error: function() {
                        //
                    }
                });

                e.preventDefault();
            });
        });
    </script>

    <script type="text/javascript">
        $(document).ready(function() {
            $(".surfCamReportLoc .swell .dir").each(function(index) {
                var i = $(this);
                i.siblings('i').rotate({ angle: getAngle(i.text()) });
            });

            $(".surfCamReportLoc .wind .dir").each(function(index) {
                var i = $(this);
                i.siblings('i').css({
                    "background-position": getWindDirection(i.text()) +
                        "px " +
                        getWindPower(i.siblings('.val').text().split(" ")[0]) +
                        "px"
                });
            });

            function getAngle(input) {
                var points = [
                    "N", "NNE", "NE", "ENE", "E", "ESE", "SE", "SSE", "S", "SSW", "SW", "WSW", "W", "WNW", "NW", "NNW"
                ];
                var pointSize = 360.0 / points.length;
                return pointSize * points.indexOf(input);
            }


            function getWindDirection(point) {
                var points = [
                    "S", "SSE", "SE", "ESE", "E", "ENE", "NE", "NNE", "N", "NNW", "NW", "WNW", "W", "WSW", "SW", "SSW"
                ];
                var direction = points.indexOf(point);
                var margin = 35;
                return (0 - direction * margin);
            }

            function getWindPower(power) {
                var margin = 35;
                return (0 - parseInt(power / 5) * margin);
            }
        });
    </script>

    <script type="text/javascript">
        $(".plusControlWrap .plusIndicator").hover(function(event) {
                event.preventDefault();
                if (!$(this).parents("div.plusControlWrap:first").hasClass("plusControlTriggerHover")) {
                    $(this).parents("div.plusControlWrap:first").find(".plusControlPanel:first").show();
                }

            },
            function() {
                if (!$(this).parents("div.plusControlWrap:first").hasClass("plusControlTriggerHover")) {
                    $(this).parents("div.plusControlWrap:first").find(".plusControlPanel:first").hide();
                }

            });

        $(".plusControlPanel").hover(function(event) {
                event.preventDefault();
                if (!$(this).parents("div.plusControlWrap:first").hasClass("plusControlTriggerHover")) {
                    $(this).show();
                }
            },
            function() {
                if (!$(this).parents("div.plusControlWrap:first").hasClass("plusControlTriggerHover")) {
                    $(this).hide();
                }

            });
    </script>


    <script type="text/javascript">
        jQuery(document).ready(function() {
            var co = $("#modalCameraOutage");
            var lc = $("#modalLockedContent");

            if (co.length) {
                jQuery(co).overlay({
                    mask: { color: '#152f51', loadSpeed: 200, opacity: 0.5 },
                    fixed: false,
                    closeOnClick: true,
                    load: false
                });
                jQuery(co).overlay().load();
            } else if (lc.length) {
                jQuery(lc).overlay({
                    mask: { color: '#152f51', loadSpeed: 200, opacity: 0.5 },
                    fixed: false,
                    closeOnClick: true,
                    load: false
                });
                jQuery(lc).overlay().load();
            }
        });
    </script>
    

