            });

            $(document).ready(function () {
                if ($.cookie("cw-cclater") == null) {
                    $('#update-cc-card').show();
                }
                if ($.cookie("cw-cclater") == "1") {
                    $('#update-cc-card').hide();
                }
            });
            $("#remindmelatertab").click(function () {
                $.cookie("cw-cclater", "1", { expires: 2, path: "/" });
                $("#update-cc-card").hide();
            });
        </script>
        
