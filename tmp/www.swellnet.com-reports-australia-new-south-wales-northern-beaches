        return false;
    });
    $("#swellnet-comments-form .form-textarea").val('');
    $('#swellnet-comments-form .form-submit').attr('disabled','disabled');
    // $('#swellnet-comments-form .form-textarea').change(function(){
    //     if($(this).val != ''){
